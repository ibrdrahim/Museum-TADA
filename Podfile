# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'rijksMuseumTADA' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for rijksMuseumTADA

    pod 'RxSwift'
    pod 'RxCocoa'

  # Moya HTTP networking library
    pod 'Moya/RxSwift'
    pod 'Moya-ObjectMapper/RxSwift'
  # Realm for Local Storage
    pod 'RealmSwift'
  # SideMenu for Sidemenu Harmburger button
    pod 'SideMenu'
  # SDWebImage for Load Image
    pod 'SDWebImage', '~> 4.0'

  target 'rijksMuseumTADATests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'rijksMuseumTADAUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
